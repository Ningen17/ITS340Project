        this.ptMidInit = ptMidInit;
    }

    public void setPtPreviousLastName(String ptPreviousLastName) {
        this.ptPreviousLastName = ptPreviousLastName;
    }

    public void setPtSocial(String ptSocial) {
        this.ptSocial = ptSocial;
    }

    public void setSexualActive(String sexualActive) {
        this.sexualActive = sexualActive;
    }

    public void setSubRelation(String subRelation) {
        this.subRelation = subRelation;
    }
}
